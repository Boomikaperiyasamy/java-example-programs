import java.util.Scanner;
class Fact{
public static void main(String args[])
{
    Scanner j=new Scanner(System.in);
    System.out.println("Enter a num:");
    int d= j.nextInt();
    double f=1;
    for (int i=1;i<=d;i++)
    {
         f=f*i;
    }
    System.out.println("The factorial is :"+f);
}
}
